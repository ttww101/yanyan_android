<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:id="@android:id/background" >	
		<shape>
			<corners android:radius="0dp" />
			<gradient android:angle="270" android:centerColor="#efefef"
				android:centerY="0.75" android:endColor="#a3a3a3"
				android:startColor="#c7c7c7" />
		</shape>
	</item>

	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="0dp" />
				<gradient android:angle="270" android:centerColor="#ff5a5d5a"
					android:centerY="0.75" android:endColor="#ff747674"
					android:startColor="#ff9d9e9d" />
			</shape>
		</clip>
	</item>

	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="0dp" />
				<gradient android:angle="270" android:centerColor="#fd9a4a"
					android:centerY="0.75" android:endColor="#fd9a4a"
					android:startColor="#fd9a4a" />
			</shape>
		</clip>
	</item>

</layer-list>